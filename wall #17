   line(100,100,200,100)
  line(100,300,200,300);
  line(100,50,200,50);
  line(100,150,200,150);
  line(100,250,200,250);
  line(100,200,200,200);
  line(200,0,200,400);
  line(205,0,205,400);
  line(210,0,210,400);
  line(215,0,215,400);
  line(220,0,220,400);
  line(225,0,225,400);
  line(230,0,230,400);
  line(235,0,235,400);
  line(240,0,240,400);
  line(245,0,245,400);
  line(250,0,250,500);
  line(255,0,255,400);
  line(260,0,260,400);
  line(265,0,265,400);
  line(270,0,270,400);
  line(275,0,275,400);
  line(280,0,280,400);
  line(285,0,285,400);
  line(290,0,290,400);
  line(295,0,295,400);
  line(300,0,300,400);
    line(250,0,250,400);
  strokeWeight(4);
  line(300,300,400,100);
  line(300,350,400,150);
  line(350,400,400,300);
  line(325,400,400,250)
